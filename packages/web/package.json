{
  "name": "@package/web",
  "version": "0.0.1",
  "license": "GPL-3.0",
  "author": "Elias Mawa <elias@emawa.io>",
  "homepage": "https://github.com/celestialstag/react-directus-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/celestialstag/react-directus-template.git",
    "directory": "packages/utilities"
  },
  "type": "module",
  "scripts": {
    "lint": "echo 'linting @package/web' && eslint .",
    "prettier": "echo 'formatting @package/web' && yarn prettier  --plugin-search-dir . --write . --check .",
    "test": "echo 'test @package/web' && yarn test:unit && yarn test:web",
    "test:unit": "echo 'test:unit @package/web' && vitest",
    "test:web": "echo 'test:web @package/web' && playwright test",
    "watch": "ts-node ./src/server.ts",
    "serve": "echo 'serving @package/web' && cross-env NODE_ENV=development yarn watch",
    "optimize": "echo 'optimize @package/web:client' && cross-env NODE_ENV=production vite optimize",
    "build": "echo 'build @package/web:client' && cross-env NODE_ENV=production vite build",
    "preview": "echo 'preview @package/web' && cross-env NODE_ENV=production yarn vite preview",
    "start": "echo 'starting @package/web' && cross-env NODE_ENV=production yarn watch"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "~1.2.0",
    "@emotion/react": "~11.10.5",
    "@emotion/styled": "~11.10.5",
    "@lib/config": "~0.0.1",
    "@lib/shared": "~0.0.1",
    "@lib/stande": "~0.0.1",
    "@lib/stores": "~0.0.1",
    "@mantine/core": "~6.0.5",
    "@mantine/form": "~6.0.8",
    "@mantine/hooks": "~6.0.5",
    "@playwright/test": "~1.28.1",
    "compression": "~1.7.4",
    "date-fns": "^2.29.3",
    "evergreen-ui": "~7.1.5",
    "express": "~4.18.2",
    "immer": "~9.0.21",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-icons": "~4.8.0",
    "sirv": "~2.0.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "~11.10.5",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "@types/react": "~18.0.10",
    "@types/react-dom": "~18.0.10",
    "@vitejs/plugin-react": "~2.0.1",
    "sass": "~1.56.2",
    "ts-node": "~10.9.1",
    "typescript": "~5.0.2",
    "vite": "~4.2.1",
    "vite-plugin-ssr": "^0.4.60"
  }
}
